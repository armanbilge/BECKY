				<microsatelliteSamplerTreeLikelihood idref="Locus3.microsatelliteSamplerTreeLikelihood"/>
				<microsatelliteSamplerTreeLikelihood idref="Locus4.microsatelliteSamplerTreeLikelihood"/>
